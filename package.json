    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "utf-8-validate": "^5.0.2",
    "webpack-bundle-analyzer": "^3.5.1",
    "webpack-cli": "^3.1.0",
    "ws": "^7.1.2",
    "xhr2": "^0.2.0"
